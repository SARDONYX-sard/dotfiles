{
  "[gitconfig]": {
    "editor.semanticHighlighting.enabled": true
  },
  "[lua]": {
    "editor.defaultFormatter": "JohnnyMorganz.stylua"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnType": false,
    "editor.tabSize": 4
  },
  "cSpell.words": [
    "AllLib",
    "antialiasing",
    "autoclean",
    "autohotkey",
    "autokey",
    "autopep",
    "azfunc",
    "bbwe",
    "cblister",
    "cblisters",
    "CFUNCTYPE",
    "cmake",
    "codespell",
    "colortool",
    "commandline",
    "concfg",
    "corepack",
    "ctags",
    "cython",
    "dein",
    "Dpkg",
    "endregions",
    "ensurepath",
    "executiontime",
    "foregroundcolor",
    "fprettify",
    "fullpath",
    "ginit",
    "gmay",
    "grayscale",
    "gvim",
    "hashtables",
    "HHMMSS",
    "HWND",
    "ignorecase",
    "inetutils",
    "innounp",
    "keygen",
    "keyhac",
    "lessmsi",
    "luacheck",
    "luarocks",
    "mintty",
    "minttyrc",
    "modcache",
    "mspaint",
    "msys",
    "MYGVIMRC",
    "myruntime",
    "MYVIMRC",
    "navi",
    "neovim",
    "nologo",
    "nonewline",
    "noreport",
    "nvim",
    "pacman",
    "pipx",
    "pnpm",
    "powerline",
    "procs",
    "pshazz",
    "pwsh",
    "pyauto",
    "pyenv",
    "quotepath",
    "Readline",
    "ripgrep",
    "rlwrap",
    "roundrock",
    "rustc",
    "rustfilt",
    "safecrlf",
    "shellcheck",
    "shfmt",
    "Shougo",
    "solargraph",
    "sonictemplate",
    "spellfile",
    "stylua",
    "sysstat",
    "tolf",
    "treee",
    "useb",
    "venv",
    "vimrc",
    "VIMRUNTIME",
    "wekyb",
    "wsltty",
    "zoxide"
  ],
  "editor.rulers": [80],
  "editor.tabCompletion": "on",
  "files.associations": {
    ".editorconfig": "properties",
    ".envpath": "shellscript",
    ".gitconfig": "gitconfig",
    ".gitignore": "ignore",
    ".luacheckrc": "lua",
    "*.json": "jsonc",
    "*.txt": "txt",
    "*.vim": "viml",
    "git-config.txt": "gitconfig",
    "LICENSE": "txt"
  },
  "files.exclude": {
    "**/__pycache__": true,
    "**/.idea": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true
  },
  "files.watcherExclude": {
    "**/__pycache__": true,
    "**/.git/objects/**": true,
    "**/.idea": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/.venv": true
  },
  "Lua.codeLens.enable": true,
  "Lua.hint.enable": true,
  "Lua.hint.setType": true,
  "python.analysis.typeCheckingMode": "strict",
  "python.envFile": "${workspaceFolder}/.env",
  "python.formatting.provider": "none",
  "python.linting.enabled": true,
  "python.linting.lintOnSave": true,
  "python.linting.pycodestyleEnabled": false
}
